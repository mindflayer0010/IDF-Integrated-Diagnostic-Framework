# Copy this file to .env and set your values
PORT=4000
# For local MongoDB
MONGO_URI=mongodb://127.0.0.1:27017/caremate
# For Atlas (replace with your connection string)
# MONGO_URI=mongodb+srv://<user>:<pass>@<cluster-host>/<db>?retryWrites=true&w=majority
ALLOW_ORIGIN=http://localhost:5173

# AI Providers (optional)
# Primary (Gemini)
GEMINI_API_KEY=
# Fallback (OpenAI)
OPENAI_API_KEY=
# Optional: override OpenAI model
OPENAI_MODEL=gpt-4o-mini
